{
    "name": "@lunarcloud/sta-play-webapp",
    "private": true,
    "type": "module",
    "engines": {
        "node": ">=20.19.0"
    },
    "scripts": {
        "eslint-fix": "npx eslint --ext .js . --fix",
        "eslint": "npx eslint --ext .js .",
        "htmllint": "npx linthtml \"**/*.html\"",
        "csslint": "npx stylelint \"**/*.css\" --quiet-deprecation-warnings",
        "csslint-fix": "npx stylelint \"**/*.css\" --quiet-deprecation-warnings --fix",
        "lint": "npm run eslint; npm run htmllint; npm run csslint",
        "lint-fix": "npm run eslint-fix; npm run htmllint; npm run csslint-fix",
        "test": "npx web-test-runner",
        "test:watch": "npx web-test-runner --watch",
        "test:coverage": "npx web-test-runner --coverage",
        "serve": "serve",
        "copy-deps": "node copy-deps.js",
        "cem": "npx cem analyze"
    },
    "dependencies": {
        "@google/model-viewer": "^4.1.0",
        "@ungap/custom-elements": "^1.3.0",
        "fflate": "^0.8.2",
        "idb": "^8.0.3"
    },
    "devDependencies": {
        "@custom-elements-manifest/analyzer": "^0.11.0",
        "@esm-bundle/chai": "^4.3.4-fix.0",
        "@linthtml/linthtml": "^0.10.2",
        "@web/test-runner": "^0.20.2",
        "@web/test-runner-playwright": "^0.11.1",
        "custom-element-vs-code-integration": "^1.5.0",
        "eslint": "^9.39.2",
        "eslint-plugin-editorconfig": "^4.0.3",
        "eslint-plugin-jsdoc": "^62.5.5",
        "neostandard": "^0.12.2",
        "npm-check-updates": "^19.3.2",
        "serve": "^14.2.5",
        "stylelint": "^17.3.0",
        "stylelint-config-standard": "^40.0.0"
    },
    "needs-compatible-version": {
        "eslint-plugin-editorconfig": "^4.0.3"
    }
}
