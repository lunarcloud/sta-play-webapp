{
    "name": "@lunarcloud/sta-play-webapp",
    "private": true,
    "scripts": {
        "eslint-fix": "npx eslint --ignore-path .gitignore --ext .js . --fix",
        "eslint": "npx eslint --ignore-path .gitignore --ext .js .",
        "htmllint": "npx linthtml \"**/*.html\"",
        "csslint": "npx stylelint \"**/*.css\" --quiet-deprecation-warnings",
        "csslint-fix": "npx stylelint \"**/*.css\" --quiet-deprecation-warnings --fix",
        "lint": "npm run eslint && npm run htmllint && npm run csslint",
        "serve": "serve"
    },
    "devDependencies": {
        "@linthtml/linthtml": "^0.9.5",
        "eslint": "^8.55.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-editorconfig": "^4.0.3",
        "eslint-plugin-import": "^2.29.0",
        "eslint-plugin-jsdoc": "^46.9.0",
        "eslint-plugin-n": "^16.4.0",
        "eslint-plugin-promise": "^6.1.1",
        "idb": "^8.0.0",
        "serve": "^14.2.1",
        "stylelint": "^16.0.2",
        "stylelint-config-standard": "^35.0.0"
    }
}
